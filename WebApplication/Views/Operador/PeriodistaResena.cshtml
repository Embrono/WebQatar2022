@{
}
@using Dominio
@model List<Periodista>


@if(Model != null && Model.Count > 0)
{
    foreach (Periodista periodista in Model)
    {
        
        <div class="alert alert-primary" role="alert">
           <p>@periodista.Nombre , @periodista.Apellido</p> 
           <a href="~/Operador/PeriodistaResenaId/@periodista.Email" class="btn btn-primary" name="email">Mira Sus reseñas</a>.
        </div>
    }
}