@using Dominio
@using Microsoft.AspNetCore.Http;
@model List<Partido>;

<div class="container">
    @foreach (Partido p in Model)
    {
        <div class="alert alert-primary" role="alert">
            <h4 class="alert-heading">@p.ResultadoFinal</h4>
            <hr>
            <p class="mb-0"> @p.FechaYHora</p>
            <p class="mb-0"> SELECCION LOCAL: @p.SeleccionLocal.Pais.Nombre  SELECCO VISITANTE: @p.SeleccionVisitante.Pais.Nombre </p>

        </div>
    }
</div>