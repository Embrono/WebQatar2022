@using Dominio;
@model List<Resena>;
@if(Model!=null&& Model.Count>0 ){

@foreach(Resena r in Model){
<div class="alert alert-primary" role="alert">
        <h4 class="alert-heading">@r.Titulo</h4>
        <hr>
        <p>@r.Contenido</p>
        <hr>
        <p>@r.Partido.SeleccionLocal.Pais.Nombre</p>
        <p>@r.Partido.SeleccionVisitante.Pais.Nombre</p>
</div>
}
}else {
    <div class=" alert alert-danger" >
        <h4 class="alert-heading">NO TIENE RESENAS</h4>
    </div>
}
