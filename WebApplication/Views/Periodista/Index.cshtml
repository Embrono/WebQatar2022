@{
}
@using Microsoft.AspNetCore.Http;
@using Dominio

@if (ViewBag.Error != null)
{
    <div class="alert alert-danger">
        @ViewBag.Error
    </div>
}

<div class="alert alert-success" role="alert">
    <h4 class="alert-heading">Bienvenido</h4>
    <p>@ViewBag.Email</p>
    <hr>
    <p class="mb-0">Periodista</p>
</div>

